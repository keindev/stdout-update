{
    "name": "stdout-update",
    "version": "1.5.0",
    "description": "Updates the previous output in the terminal. Useful for correct rendering progress bars, animations, etc.",
    "homepage": "https://github.com/keindev/stdout-update#readme",
    "license": "MIT",
    "author": "Daniil Ryazanov <kein@tagproject.ru>",
    "keywords": [
        "log",
        "logger",
        "logging",
        "shell",
        "update",
        "refresh",
        "overwrite",
        "output",
        "cli",
        "stdout",
        "update",
        "terminal",
        "term",
        "console",
        "indicator",
        "progress"
    ],
    "main": "lib/update-manager.js",
    "types": "lib/update-manager.d.ts",
    "unpkg": "lib/update-manager.js",
    "module": "lib/update-manager.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/keindev/stdout-update.git"
    },
    "bugs": {
        "url": "https://github.com/keindev/stdout-update/issues"
    },
    "scripts": {
        "prepare:clean": "rimraf lib coverage",
        "check:lint": "eslint src/**/*.ts --fix",
        "check:spell": "cspell -c .vscode/cspell.json --no-summary \"*.*\" \"**/*.ts\" \"**/*.js\"",
        "check:test": "jest",
        "generate:ghinfo": "ghinfo generate -d media -t utils",
        "build:ts": "tsc",
        "build": "npm-run-all prepare:* generate:* check:* build:*",
        "util:changelog": "changelog generate",
        "util:commit-msg": "changelog lint --message HUSKY_GIT_PARAMS --lowercase-only",
        "util:pre-commit": "npm-run-all check:*"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "dependencies": {
        "ansi-escapes": "^4.3.1",
        "ansi-styles": "^4.2.1",
        "string-width": "^4.2.0",
        "strip-ansi": "^6.0.0"
    },
    "devDependencies": {
        "@types/ansi-styles": "^3.2.1",
        "@types/jest": "^26.0.0",
        "@types/node": "^14.0.12",
        "@typescript-eslint/eslint-plugin": "^4.0.0",
        "@typescript-eslint/parser": "^3.2.0",
        "changelog-guru": "^1.0.0",
        "cspell": "^4.0.63",
        "eslint": "^7.2.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-config-prettier": "^7.0.0",
        "eslint-plugin-import": "^2.21.1",
        "eslint-plugin-jest": "^23.13.2",
        "ghinfo": "^1.0.6",
        "husky": "^4.2.5",
        "jest": "^26.0.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.1.0",
        "typescript": "^4.0.2"
    }
}
